{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "webRequest-demo",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources" : ["sample.jpg","foo.html"],
  "background": {
    "scripts": ["search.js","background.js"]
  }

}