{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "Intelligent Blocker",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources" : ["src/foo_http.html","src/foo.html"],
  "background": {
    "scripts": ["./src/search.js","./src/tmpWhitelist.js","./src/blacklist_posibility.js","background.js"]
  }

}
