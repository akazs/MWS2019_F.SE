{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "Intelligent Blocker",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources" : ["foo_http.html","foo.html","caution-01.png"],
  "background": {
    "scripts": ["search.js","tmpWhitelist.js","background.js"]
  }

}
